{
  "name": "rpgbaker",
  "version": "1.2.2",
  "description": "minimalist and useless game engine (with rpg elements)",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "oldbuild": "babel src --out-dir lib",
    "oldwatch": "babel src --out-dir lib --watch",
    "olddocs": "jsdoc  src -r src/README.md -u ./tutorials  -d docs",
    "docs": "compodoc --includes ./tutorials -p ./tsconfig.json -d ./docs "
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DomDumont/rpgbaker.git"
  },
  "keywords": [
    "game"
  ],
  "author": "Dominique Dumont",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DomDumont/rpgbaker/issues"
  },
  "homepage": "https://github.com/DomDumont/rpgbaker#readme",
  "dependencies": {
    "@types/debug": "0.0.31",
    "debug": "^4.1.0",
    "howler": "^2.0.15",
    "pixi.js": "^4.8.2"
  },
  "devDependencies": {
    "@types/pixi.js": "^4.8.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jsdoc": "^3.5.5"
  }
}
